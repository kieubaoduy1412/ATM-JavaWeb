$('#continue').click(function() {
	var exp = '${transactionModel.currentPage}';
	if (exp == "gotoAddMoney") {
		window.location.href = exp;
	} else if (exp == "sendMoneyPage") {
		window.location.href = "gotoSendMoney";
	} else {
		window.location.href = "gotoGetMoney";
	}
});

$('#backWelcome').click(function() {
	window.location.href = "gotoWelcomePage";
})