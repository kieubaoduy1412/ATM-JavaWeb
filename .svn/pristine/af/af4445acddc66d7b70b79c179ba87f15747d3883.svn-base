function login(){
	$('#loginFail').text('').show();
	var userName =  $('input[name=username]').val();
	var pass =  $('input[name=password]').val();
	if (!userName) {
		if(!pass){
			$('#loginFail').text('AccountID and Password is required!').show();
		} else {
			$('#loginFail').text('AccountID is required!').show();
		}
		return;
	}
	if (!pass) {
		$('#errmsgPass').text('Password is required!').show().fadeOut(2000);
		return;
	}
	$.ajax({
		type : 'POST',
		url : 'login',
		data:{username : userName,pass : pass},
		//dataType : "json"
	}).success(function(data) {
		if(data.trim() == "welcome"){
			window.location.href = "home";
		}else{
			var result = JSON.parse(data);
			var message = result.infCustomer.message;
			$("#loginFail").html(message);
		}
    });
}