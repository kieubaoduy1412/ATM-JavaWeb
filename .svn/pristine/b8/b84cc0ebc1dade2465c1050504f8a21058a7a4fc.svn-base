body{
	background-color:#eaf0f2;
	text-align: center;
}
.successMessage{
	text-align: center;
	font-family: sans-serif;
	font-style: italic;
	font-weight: 100;	
}

@media(min-width:500px){
	#backWelcome{	
	width: 200px;
	float:right;
	color: silver;
	}
	 #continue{
	width: 200px;
	float:left;
	color: silver;
 	}
}
@media(max-width:500px){
	#backWelcome{	
		width: 100px;
		float:right;
		color: silver;
	}
	 #continue{
	width: 100px;
	float:left;
	color: silver;
 	}
}

