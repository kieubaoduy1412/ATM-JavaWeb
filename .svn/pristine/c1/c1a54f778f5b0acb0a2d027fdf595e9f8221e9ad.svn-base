$(document).ready(function() {
	$('.btnLogin').on('click',function(){
		var userName =  $('input[name=username]').val();
		var pass =  $('input[name=password]').val();

		$.ajax({
			type : 'POST',
			url : 'login',
			data:{username : userName,pass : pass},
		}).success(function(data) {
	        $('body').html(data)
	    });
	})
});